{
  "name": "wool-store",
  "version": "1.2.8",
  "description": "A store manager for wool",
  "main": "index.js",
  "scripts": {
    "pretest": "npm run lint",
    "test": "tape test/**/*.spec.js |tap-summary --no-progress",
    "autotest": "nodemon -e js,yml -x 'tape test/**/*.spec.js |tap-summary --no-progress'",
    "autolinttest": "nodemon -e js,yml -x 'npm run lint && tape test/**/*.spec.js |tap-summary --no-progress'",
    "coverage": "nyc tape test/**/*.spec.js && nyc report --reporter=html",
    "lint": "eslint index.js test/",
    "autolint": "nodemon -x eslint index.js test/",
    "preversion": "npm test && npm outdated"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicocube/wool-store.git"
  },
  "keywords": [
    "state"
  ],
  "author": "Nicolas Lochet <nicolas.lochet@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/nicocube/wool-store/issues"
  },
  "homepage": "https://github.com/nicocube/wool-store#readme",
  "devDependencies": {
    "eslint": "^5.12.0",
    "nodemon": "^1.18.9",
    "nyc": "^13.1.0",
    "tap-summary": "^4.0.0",
    "tape": "^4.9.2",
    "tape-async": "^2.3.0"
  }
}
